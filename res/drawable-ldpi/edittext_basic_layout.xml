<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android"><item android:state_pressed="true">
    <shape android:shape="rectangle">
        <stroke android:width="5dp" android:color="#5000CC00" />
        <padding android:left="10dp" android:top="1dp" android:right="10dp"
            android:bottom="1dp" />
    </shape>
</item>
<item android:state_focused="true">
    <shape>
        <stroke android:width="5dp" android:color="#5000CC00" />
        <padding android:left="10dp" android:top="1dp" android:right="10dp"
            android:bottom="1dp" />
    </shape>
</item>
<item>
    <shape>
        <stroke android:width="5dp" android:color="#50F9F8F8" />
        <padding android:left="10dp" android:top="1dp" android:right="10dp"
            android:bottom="1dp" />
    </shape>
</item></selector>